<p style="color: green"><%= notice %></p>

<h1>Properties</h1>

<div id="properties">
  <% @properties.each do |property| %>
    <%= render property %>
    <p>
      <%= link_to "Show this property", property %>
      <%= link_to "Show All Bookings",all_bookings_property_path(property.id)%>
    </p>
  <% end %>
</div>

<%= link_to "New property", new_property_path %>
