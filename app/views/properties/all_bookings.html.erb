<div id="bookings">

<table>
  <thead>
    <tr>
      <th>Price</th>
      <th>Start Date</th>
      <th>End Date</th>
      <th>Status</th>
     </tr>
  </thead>

  <% @bookings.each do |booking| %>
  <tbody>
    <tr>
      <td><%=booking.price%></td>
      <td><%=booking.start_date%></td>
      <td><%=booking.end_date%></td>
      <td><%=booking.status%></td>
      <td><%= link_to "Show this booking", booking %> </td>
    </tr> 
  </tbody>
  <% end %>
</table>

