<div id="properties">

</div>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Address</th>
      <th>Latitude</th>
      <th>Longitude</th>
      <th>Price</th>
     </tr>
  </thead>

  <% @properties.each do |property| %>
  <tbody>
    <tr>
      <td><%=property.name%></td>
      <td><%=property.address%></td>
      <td><%=property.latitude%></td>
      <td><%=property.longitude%></td>
      <td><%=property.price%></td>
      <td> <%= link_to "Booking", new_booking_path(property_id: property.id)%> </td>
    </tr> 
  </tbody>
  <% end %>
</table>

 